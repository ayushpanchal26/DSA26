   "source": [
    "def insertion(arr,n):\n",
    "    for i in range(n):\n",
    "        j = i\n",
    "        while(j>0 and arr[j-1]>arr[j]):\n",
    "            arr[j-1],arr[j] = arr[j],arr[j-1]\n",
    "            j-=1\n",
    "\n",
    "    return arr"
   ]
